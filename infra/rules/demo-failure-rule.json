{
  "type": "query",
  "index": [
    "apm-*-transaction*",
    "traces-apm*",
    "auditbeat-*",
    "endgame-*",
    "filebeat-*",
    "logs-*",
    "packetbeat-*",
    "winlogbeat-*"
  ],
  "filters": [],
  "language": "kuery",
  "query": "labels.ci_pipeline_name : \"AntiFraud » antifraud » demo\" and not labels.ci_pipeline_run_result : \"SUCCESS\" ",
  "author": [],
  "false_positives": [],
  "references": [],
  "risk_score": 21,
  "risk_score_mapping": [],
  "severity": "critical",
  "severity_mapping": [],
  "threat": [],
  "name": "demo job failure",
  "description": "This Alert is triggered when the demo job not success",
  "tags": ["demo", "pipeline", "failure"],
  "license": "",
  "interval": "10s",
  "from": "now-70s",
  "to": "now",
  "meta": {
    "from": "1m",
    "kibana_siem_app_url": "http://localhost:5601/app/security"
  },
  "actions": [
    {
      "group": "default",
      "id": "CONNECTOR_ID",
      "params": {
        "message": "Rule {{context.rule.name}} generated {{state.signals_count}} alerts\n\n{{context.rule.query}}"
      },
      "action_type_id": ".slack"
    }
  ],
  "enabled": true,
  "throttle": "rule"
}
